{
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "declaration": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2019",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "incremental": true,
    "esModuleInterop": true,
    "include": ["**/*.ts"],
    "exclude": [
      "node_modules",
      "dist"]
  }
}
